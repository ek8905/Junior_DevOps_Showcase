apiVersion: v1
clusters:
- cluster:
    certificate-authority: "{{ ca_cert }}" 
    server: "https://{{ master_ip }}:6443"
  name: bootstrap
contexts:
- context:
    cluster: bootstrap
    user: kubelet-bootstrap
  name: bootstrap
current-context: bootstrap
kind: Config
preferences: {}
users:
- name: kubelet-bootstrap
  user:
    token: "{{ bootstrap_token_id }}.{{ bootstrap_token_secret }}"
